{
  "manifest_version": 2,

  "name": "Youtube Media Keys",
  "description": "Youtube Hardware Media Keys Support for next and previous video",
  "version": "1.0.0",
  "icons": { "128": "icon_128.jpg" },


  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },


  "permissions": ["activeTab", "tabs"],


  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/watch*"],
      "js": ["injected.js"]
    }
  ]
}